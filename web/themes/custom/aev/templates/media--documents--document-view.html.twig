{#
/**
 * @file
 * Default theme implementation to present a media entity.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
<article{{ attributes.addClass(classes, 'col-xs-12') }}>
  {% if content %}
    <div class="col-xs-1 document-thumbnail">{{ content.field_image }}</div>
    <div class="col-xs-11">{{ content.field_file }}</div>
  {% endif %}
</article>
